    "type": "library",
    "require": {
        "kreait/laravel-firebase": "^5.9"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mjakubowski\\FirebaseAuth\\": "src/"
        }
    },
    "extra": {
